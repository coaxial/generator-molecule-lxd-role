{
  "name": "generator-molecule-lxd-role",
  "version": "0.0.0",
  "description":
    "Ansible role testing with molecule using LXD instead of Docker",
  "homepage": "",
  "author": {
    "name": "coaxial",
    "email": "hi@64b.it",
    "url": "https://64b.it"
  },
  "files": ["generators"],
