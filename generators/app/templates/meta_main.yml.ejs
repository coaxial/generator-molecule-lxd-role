---
galaxy_info:
  author: <%- authorName %>
  description: <%- roleDesc %>
<% if(authorOrganization !== '') { -%>
  company: <%- authorOrganization %>
<% } -%>
  role_name: <%- roleName %>
  license: <%- license %>
  min_ansible_version: <%- minAnsibleVer %>
  platforms:
<%- targetVersions %>
<%- galaxyTags %>
<% if(hasDeps) { -%>
  dependencies:
<% roleDeps.forEach(dep => { -%>
    - { role: <%- dep %> }
<% }) -%>
<% } else { -%>
  dependencies: []
<% } -%>
